{
  "name": "nx-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --all --parallel --maxParallel=20",
    "build": "nx run-many --target=build --all --parallel --maxParallel=20",
    "test": "nx test",
    "clean": "rimraf node_modules/.cache/nx dist",
    "commit": "git-cz",
    "release": "standard-version -a",
    "postintall": "husky install",
    "frontend:serve": "nx run-many --target=serve --projects=frontend-main --parallel --maxParallel=20",
    "backend:serve": "nx run-many --target=serve --projects=backend-main --parallel --maxParallel=20",
    "codegen": "graphql-codegen --config codegen.config.yml -w"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.9",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-graphql-request": "^4.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-react-apollo": "^3.2.11",
    "@graphql-codegen/typescript-urql": "^3.5.7",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.10.1",
    "@nrwl/cypress": "13.10.1",
    "@nrwl/eslint-plugin-nx": "13.10.1",
    "@nrwl/jest": "13.10.1",
    "@nrwl/linter": "13.10.1",
    "@nrwl/nest": "^13.10.1",
    "@nrwl/next": "^13.10.1",
    "@nrwl/node": "13.10.1",
    "@nrwl/react": "13.10.1",
    "@nrwl/web": "13.10.1",
    "@nrwl/workspace": "13.10.1",
    "@nx-tools/nx-docker": "^2.3.0",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "27.2.3",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cypress": "^9.1.0",
    "cz-customizable": "^6.3.0",
    "eslint": "~8.12.0",
    "eslint-config-next": "12.1.2",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "nx": "13.10.1",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "react-test-renderer": "18.0.0",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2"
  },
  "engines": {
    "node": "v16.14.2",
    "npm": "8.5.0"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    },
    "scripts": {
      "postbump": "gitmoji-changelog update",
      "precommit": "git add CHANGELOG.md"
    }
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nrwl/next": "13.10.1",
    "core-js": "^3.6.5",
    "next": "12.1.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tslib": "^2.0.0",
    "urql": "^2.2.0"
  }
}
